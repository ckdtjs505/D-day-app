{
  "name": "admin",
  "version": "1.0.0",
  "description": "d-day-app admin",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:server": "nodemon --exec babel-node ./src/app.js",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:server": "babel src --out-dir build",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "copyAll": "cp -R src/views build/views && cp -R src/dist build/dist",
    "start": "node build/app.js",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prestart": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.8",
    "morgan": "^1.10.0",
    "node-schedule": "^1.3.2",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "pug": "^3.0.0",
    "sass": "^1.26.12",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.3.0",
    "node-sass": "^4.14.1",
    "css-loader": "^4.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "nodemon": "^2.0.4"
  }
}
